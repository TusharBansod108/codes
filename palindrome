class Solution {
public:
    bool isPalindrome(int x) {
        int n=0,r,t;
        t=x;
        while(x>0){
            r=x%10;
            n=n*10+r;
            x=x/10;
        }
          if(t==n){
            return true;
        }else{
            return false;
        }
        
    } 
    
};
